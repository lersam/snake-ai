{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "chatbot_copilot.app:app",
                "--reload",
                "--port",
                "8000"
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "ChatBoat",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            // "args": [
            //     "runserver",
            //     "8000"
            // ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}